{
  "orm": {
    "type": "sqlite",
    "database": ":memory:",
    "logger": "file",
    "logging": [
      "query",
      "error"
    ],
    "maxQueryExecutionTime": 90,
    "synchronize": true
  },
  "localAuth": {
    "usernameField": "username",
    "passwordField": "password",
    "session": true
  },
  "webserver": {
    "httpPort": 8000,
    "httpsPort": 8001,
    "secret": "super_secret_key",
    "viewPath": "views",
    "viewEngine": "ejs",
    "staticPath": "static"
  },
  "test": {
    "fixture": {
      "username": "admin",
      "password": "admin",
      "displayName": "admin"
    }
  },
  "sessionstore": {}
}