{
  "orm": {
    "type": "sqlite",
    "database": "db/production.db",
    "logger": "file",
    "logging": [
      "error"
    ],
    "maxQueryExecutionTime": 90,
    "synchronize": true
  },
  "localAuth": {
    "usernameField": "username",
    "passwordField": "password",
    "session": true
  },
  "webserver": {
    "httpPort": 80,
    "httpsPort": 443,
    "secret": "super_secret_key_pro",
    "viewPath": "views",
    "viewEngine": "ejs",
    "staticPath": "static"
  },
  "sessionstore": {}
}