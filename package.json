{
  "name": "skilltree",
  "version": "1.0.4",
  "description": "Skill Tree with ReactJs, Bootstrap 4, EJS frontend and Express, SPDY backend",
  "main": "production/index.js",
  "author": "David Janos Zarandi <david.zarandi@nokia.com>",
  "license": "BSD-3-Clause",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/nokia/skilltree"
  },
  "scripts": {
    "bundle": "webpack --config webpack.config.prod.js",
    "bundle:debug": "webpack --config webpack.config.js --watch",
    "pretest": "concurrently \"tsc --p tsconfig.test.json\" \"tsc --p tsconfig.dev.json\" \"webpack --config webpack.config.js\"",
    "test": "NODE_ENV=test NODE_TLS_REJECT_UNAUTHORIZED=0 concurrently --kill-others \"node debug/index.js\" \"jest\"",
    "prebuild": "tslint ./tests/source/*",
    "build": "tsc --p tsconfig.dev.json --watch",
    "debug": "NODE_ENV=development nodemon --inspect debug/index.js",
    "prestart": "tsc --p tsconfig.prod.json",
    "start": "NODE_ENV=production node production/index.js"
  },
