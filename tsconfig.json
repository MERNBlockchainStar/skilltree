{
  "compilerOptions": {
    "jsx": "react",
    "lib": [
      "es5",
      "dom",
      "webworker"
    ],
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "./static/",
    "sourceMap": true,
    "target": "es5",
    "skipLibCheck": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true
  },
  "include": [
    "tsx/**/*"
  ],
  "exclude": [
    "source/**/*",
    "node_modules/**/*"
  ]
}